@model List<WilliamHill.Service.Risk.UnSettled.UnSettledRisk>
@{
    ViewBag.Title = "Unsettled Bets Risk Analysis";
}

<h2>Unsettled Bets</h2>
<p>Unsettled Bets showing risk</p>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Bet ID</th>
            <th>Concern</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.BetId</td>
                <td>@customer.Concern</td>
            </tr>               
        }
    </tbody>
</table>

