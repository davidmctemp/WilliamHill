@model List<WilliamHill.Service.Risk.UnSettled.UnSettledRisk>
@{
    ViewBag.Title = "Unsettled Bets Risk Analysis";
}

<section class="success" id="about">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h2>Unsettled</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6">
                <p>Details of Unsettled Bets - </p>
                <ul>
                    @foreach (var customer in Model)
                    {
                        <li>@customer.Concern</li>
                    }
                </ul>
            </div>
        </div>
    </div>
</section>
