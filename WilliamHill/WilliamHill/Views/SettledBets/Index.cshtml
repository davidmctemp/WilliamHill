@model List<WilliamHill.Service.Risk.UnSettled.SettledRisk>
@{
    ViewBag.Title = "Settled Bets Risk Analysis";
}

<section class="success" id="about">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h2>Settled Bets</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <p>The Following Customers have been having a lot of luck lately....</p>
                <ul>
                    @foreach (var customer in Model)
                    {
                        <li>@customer.Concern</li>
                    }
                </ul>
            </div>
        </div>
    </div>
</section>
